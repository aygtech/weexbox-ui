{
  "name": "weexbox-ui",
  "version": "0.0.1",
  "description": "一款基于weex-ui封装的可自由定制的UI组件库，搭配weexbox食用更佳",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "example": "example"
  },
  "scripts": {
    "build": "npm run build:entry && webpack --progress",
    "build:entry": "node bin/index.js",
    "watch": "webpack --progress --watch",
    "serve": "anywhere -d build/example/ -p 8888",
    "start": "npm run build && (npm run serve & npm run watch)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WeexBox/ui.git"
  },
  "keywords": [
    "weex",
    "weex-ui",
    "weexbox"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WeexBox/ui/issues"
  },
  "homepage": "https://github.com/WeexBox/ui#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "iOS 7",
    "iOS 9"
  ],
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "url-parse": "^1.4.3",
    "weex-bindingx": "^0.0.48",
    "weex-ui": "^0.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-stage-0": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.8.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^4.7.1",
    "filemanager-webpack-plugin": "^2.0.5",
    "glob": "^7.1.3",
    "lerna": "^3.4.3",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "os": "^0.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-plugin-weex": "^0.1.7",
    "postcss-px-to-viewport": "^0.0.3",
    "postcss-url": "^8.0.0",
    "sass-loader": "^7.1.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.1.2",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "weex-loader": "^0.7.12",
    "weex-vue-loader": "^0.7.0",
    "weex-vue-precompiler": "^0.1.25",
    "weex-vue-render": "^1.0.33"
